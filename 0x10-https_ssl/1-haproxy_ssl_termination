global
    log 127.0.0.1 local0 notice
    maxconn 2000
    user haproxy
    group haproxy
defaults
    log     global
    mode    http
    option  httplog
    option  dontlognull
    retries 3
    option redispatch
    timeout connect  5000
    timeout client  10000
    timeout server  10000

frontend node
	default_backend servers
	bind *:443

backend servers
	mode https
	stats enable
	stats uri /haproxy?stats
	option httpclose
	balance roundrobin
	server 172785-web-01 100.25.151.158:80 check
	server 172785-web-02 34.207.57.64:80 check
